# A2A Agent Configuration
HOST=0.0.0.0
PORT=8000

# Platform Selection (GOOGLE or OPENAI)
PLATFORM=OPENAI  # Choose: GOOGLE or OPENAI

# API Keys (Set based on PLATFORM choice)
GOOGLE_API_KEY=your_google_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Model Settings (Based on PLATFORM)
# For GOOGLE:
# LLM_MODEL=gemini-1.5-flash-latest  # 50 requests/day limit
# LLM_MODEL=gemini-1.5-pro-latest  # Different quota
# LLM_MODEL=gemini-pro  # Alternative model

# For OPENAI:
# LLM_MODEL=gpt-4-turbo-preview
# LLM_MODEL=gpt-4
# LLM_MODEL=gpt-3.5-turbo  # Most cost-effective

LLM_MODEL=gpt-3.5-turbo  # Default model

# QA Settings
MAX_CONTEXT_LENGTH=4000
TEMPERATURE=0.7
TOP_K=5

# MCP Configuration
MCP_RUNNER_URL=https://mcp-host-runner.onrender.com